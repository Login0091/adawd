local Tab2 = Window:MakeTab({
	Name = "Op thing",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab2:AddSection({
	Name = "Op scripts is here"
})

Tab2:AddButton({
	Name = "Everything you need to spam",
	Callback = function()local lib = loadstring(game:HttpGet('https://raw.githubusercontent.com/TheoTheEpic/AquaLib/main/AquaLib.lua'))()

		local window = lib.createWindow("Aqua", "ello dumb skid man/girl", true) 
		
		local tab1 = window.createTab("you need")
		local section1 = tab1.createSection("everything you need is here 1", false)
		
		section1.createButton("Ice spam", function()
			print("Button Pressed!")_G.IceSpam = true
			while wait() and _G.IceSpam do
				pcall(function()
					for i, v in pairs(game.Players:GetChildren()) do
						if v.Name ~= game.Players.LocalPlayer.Name then
							game:GetService("ReplicatedStorage").Events.createIce:FireServer(v.Character.HumanoidRootPart.CFrame.p)
						end
					end
				end)
			end
		end)
		
		section1.createButton("Un ice spam", function()_G.IceSpam = false
			print("Button Pressed!")
		end)
		
		local section2 = tab1.createSection("everything you need is here 2", false)
		
		section2.createButton("Lightning spam", function()_G.LightningSpam = true
			while wait(0.1) and _G.LightningSpam do
				pcall(function()
					for i, v in pairs(game.Players:GetChildren()) do
						if v.Name ~= game.Players.LocalPlayer.Name then
							game:GetService("ReplicatedStorage").Events.createLightning:FireServer(v.Character.HumanoidRootPart.CFrame.p)
						end
					end
				end)
			end
			print("Button Pressed!")
		end)
		
		section2.createButton("Un lightning spam", function()_G.LightningSpam = false
			print("Button Pressed!")
		end)
      		print("button pressed")
  	end    
})

Tab2:AddButton({
	Name = "Free ban hammer",
	Callback = function()local function createMovingFrame()
		local screenGui = Instance.new("ScreenGui")
		screenGui.Parent = game.Players.LocalPlayer.PlayerGui
	
		local frame = Instance.new("Frame")
		frame.Size = UDim2.new(1, 0, 1, 0) -- Full screen size
		frame.BackgroundColor3 = Color3.new(1, 1, 1) -- White background
		frame.Parent = screenGui
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Size = UDim2.new(1, 0, 0.9, 0) -- Leave more space for the close button
		textLabel.Position = UDim2.new(0, 0, 0.05, 0) -- Push down the text
		textLabel.Text = "u are an idiot no one can get ban hammer lol idiot remember there have close button" -- Add the text back
		textLabel.TextColor3 = Color3.new(0, 0, 0) -- Black text color
		textLabel.BackgroundTransparency = 1 -- Transparent background
		textLabel.FontSize = Enum.FontSize.Size10 -- Smaller font size
		textLabel.Parent = frame
	
		local closeButton = Instance.new("TextButton")
		closeButton.Size = UDim2.new(0, 2, 0, 3) -- Super mini size
		closeButton.Position = UDim2.new(1, -8, 0, 2) -- Positioned at the top right corner
		closeButton.Text = "" -- Add the close button text back
		closeButton.FontSize = Enum.FontSize.Size8 -- Smaller font size for the button
		closeButton.TextColor3 = Color3.new(1, 0, 0) -- Red text color
		closeButton.BackgroundColor3 = Color3.new(0.8, 0.8, 0.8) -- Light gray background
		closeButton.BorderSizePixel = 0
		closeButton.Parent = frame
	
		closeButton.MouseButton1Click:Connect(function()
			screenGui:Destroy()
		end)
	
		local function updateFramePosition()
			local screenDimensions = game:GetService("GuiService"):GetScreenSize()
			local offsetX = math.random(-screenDimensions.X.Offset + 10, screenDimensions.X.Offset - 10)
			local offsetY = math.random(-screenDimensions.Y.Offset + 5, screenDimensions.Y.Offset - 5)
			
			local targetPosition = UDim2.new(0.5, offsetX, 0.5, offsetY)
			
			local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
			game:GetService("TweenService"):Create(frame, tweenInfo, {Position = targetPosition}):Play()
		end
	
		updateFramePosition()
	end
	
	while true do
		createMovingFrame()
		wait(1) -- Wait for 1 second before spawning the next frame
	end
	
      		print("button pressed")
  	end    
})

